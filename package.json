{
  "name": "sapui5",
  "version": "0.0.1",
  "devDependencies": {
    "@sap/ui5-builder-webide-extension": "1.0.x",
    "@sapui5/ts-types": "^1.71.28",
    "@ui5/cli": "2.2.6",
    "bestzip": "^2.2.0",
    "rimraf": "3.0.2"
  },
  "scripts": {
    "build": "npm run clean && ui5 build --include-task=generateManifestBundle generateCachebusterInfo && npm run zip",
    "zip": "cd dist && npx bestzip ../SAPUI5-content.zip *",
    "clean": "npx rimraf SAPUI5-content.zip dist"
  },
  "ui5": {
    "dependencies": [
      "@sap/ui5-builder-webide-extension"
    ]
  },
  "dependencies": {
    "@sap/approuter": "^10.2.0",
    "@sap/html5-repo-mock": "^2.0.1"
  }
}
